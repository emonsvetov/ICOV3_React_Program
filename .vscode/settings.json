{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.format.enable": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "files.exclude": {
      "**/.editorconfig": true,
      "**/.git": true,
      "**/.pretterrc": true,
      "**/*.lock": false,
      "**/artifacts": true,
      "**/cache": true,
      "**/node_modules": false
    }
}
